{
    "manifest_version": 3,
    "version": "3.0",
    "name": "Pants",
    "description": "Makes Youtube Shorts less addictive",
    "content_scripts": [
        {
            "matches": ["*://www.youtube.com/*"],
            "js": ["script.js"],
            "run_at": "document_start"
        }
    ],
    "permissions": [
        "storage"
    ],
    "action":{
        "default_icon": "icon.png",
        "default_title": "Pants",
        "default_popup": "popup.html"
    }
}

