{
    "manifest_version": 2,
    "version": "1.0",
    "name": "Pants",
    "description": "Makes Youtube Shorts less addictive",
    "content_scripts": [
        {
            "matches": ["*://www.youtube.com/*"],
            "js": ["script.js"]
        }
    ],
    "permissions": [
        "webNavigation"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    }
}

